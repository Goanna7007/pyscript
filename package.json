{
    "name": "pyscript",
    "displayName": "pyscript",
    "description": "Code Highlighter for PyScript",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.67.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Hardeepsingh980/pyscript.git"
    },
    "contributes": {
        "languages": [{
            "id": "pyscript",
            "aliases": ["pyscript", "pyscript"],
            "extensions": [".html"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "pyscript",
            "scopeName": "text.html.pyscript",
            "path": "./syntaxes/pyscript.tmLanguage.json"
        }],
        "snippets": [
            {
                "language": "pyscript",
                "scopeName": "text.html.pyscript",
                "path": "./snippets/pyscript-snippets.json"
            }
        ]
    }
}